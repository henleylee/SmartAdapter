<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.henley.smartadapter.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.henley.smartadapter.demo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.henley.smartadapter.demo.ExpandableListViewActivity"
            android:label="@string/expandablelistview" />
        <activity
            android:name="com.henley.smartadapter.demo.ListViewActivity"
            android:label="@string/listview_single_multi" />
        <activity
            android:name="com.henley.smartadapter.demo.RecycleViewActivity"
            android:label="@string/recycleview_single_multi" />
    </application>

</manifest>